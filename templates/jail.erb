# DO NOT EDIT - This file is being maintained by Chef Infra

[<%= @name %>]
enabled = true
<% if @protocol -%>
protocol = <%= @protocol %>
<% end -%>
<% unless @ports.empty? -%>
port = <%= @ports.join(",") %>
<% end -%>
filter = <%= @filter %>
<% if @logpath -%>
logpath = <%= @logpath %>
<% end -%>
<% if @maxretry -%>
maxretry = <%= @maxretry %>
<% end -%>
<% if @ignoreips -%>
ignoreip = <%= @ignoreips.sort.join(" ") %>
<% end -%>
